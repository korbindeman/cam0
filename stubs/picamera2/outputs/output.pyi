class Output:
    recording: bool
    needs_pacing: bool
    def __init__(self, pts=None) -> None: ...
    def start(self) -> None: ...
    def stop(self) -> None: ...
    def outputframe(self, frame, keyframe: bool = True, timestamp=None, packet=None, audio: bool = False) -> None: ...
    def outputtimestamp(self, timestamp) -> None: ...
    @property
    def ptsoutput(self): ...
    @ptsoutput.setter
    def ptsoutput(self, file) -> None: ...
